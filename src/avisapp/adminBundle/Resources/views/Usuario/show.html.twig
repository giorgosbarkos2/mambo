{% extends "avisappadminBundle:Layers:basePrincipal.html.twig" %}
{% block title%} Ver Usuario {% endblock%}
{% block padre %}<a href="{{path('usuario')}}">Listar Usuarios</a>{% endblock %}
{% block hijo %} Ver Usuario {%endblock%}
{% block titulo %} Ver Usuario {%endblock %}
{% block contenido %}

<div class="form-horizontal">
    <div class="form-group">
        <label for="id" class="col-sm-2 control-label" >
            Id Usuario:
        </label>
        <div class="col-sm-10">
            <div name="id" class="form-control">{{ entity.id }}</div>
        </div>
    </div>
    <div class="form-group">
        <label for="usuario" class="col-sm-2 control-label">
            Usuario:
        </label>
        <div class="col-sm-10">
            <div name="usuario" class="form-control">{{ entity.usuario }}</div>
        </div>
    </div>
    <div class="form-group">
        <label for="rol" class="col-sm-2 control-label">
            Rol:
        </label>
        <div class="col-sm-10">
            <div name="rol" class="form-control">{{ entity.rol }}</div>
        </div>
    </div>
    <div class="col-sm-12">
            {{ form(delete_form) }}
    </div>
</div>

{% endblock %}
