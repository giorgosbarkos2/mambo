{% extends "avisappadminBundle:Layers:basePrincipal.html.twig" %}
{% block title%} Listar Caluga Beneficio {% endblock%}
{% block padre %}<a href="{{path('admin_calugabeneficio_new')}}">Crear Caluga Beneficio </a>{% endblock %}
{% block hijo %} Listar Caluga Beneficio {%endblock%}
{% block titulo %} Listar Caluga Beneficio {%endblock %}
{% block contenido %}
    <table class="records_list">
        <thead>
            <tr>
                <th>Titulo</th>
                <th>Descripcion</th>
                <th>Ver</th>
                <th>Editar</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.titulo }}</td>
                <td>{{ entity.descripcion }}</td>
                <td><a href="{{ path('admin_calugabeneficio_show', { 'id': entity.id }) }}" class="btn btn-success">Ver</a></td>
                <td><a href="{{ path('admin_calugabeneficio_edit', { 'id': entity.id }) }}" class="btn btn-primary" >Editar</a></td>
        {% endfor %}
        </tbody>
    </table>
    {% endblock %}
